{
  "name": "default-config",
  "baseDir": "/lib/butter/",
  "crashReporter": false,
  "cssRenderClientSide": false,
  "snapshotHTMLOnReady": false,
  "scrapePage": true,
  "backupInterval": 0,
  "title": "Popcorn Maker",
  "ui": {
    "enabled": true,
    "onLeaveDialog": true,
    "trackEventHighlight": "click"
  },
  "makeVideoURLsUnique": true,
  "mediaDefaults": {
    "frameAnimation": true
  },
  "trackEvent": {
    "defaultDuration": 5
  },
  "plugin": {
    "plugins": [
      {
        "type": "skip",
        "icon": "{{baseDir}}assets/plugins/skip/skip-icon.png",
        "targets": ["target-0"]
      },
      {
          "type": "mutePlugin",
          "displayName": "Mute",
          "icon": "{{baseDir}}/../../hummedia-popcorn-plugins/mutePlugin/mute.png",
          "targets": ["target-0"]
      },
      {
        "type": "youtube-search",
        "displayName": "YouTube Search",
        "targets": ["target-4"],
        "googleKeyProperty": "key"
      },
      {
        "type": "freebase-search",
        "displayName": "Freebase Search",
        "targets": ["target-4"],
        "googleKeyProperty": "key",
        "icon": "{{baseDir}}/../../hummedia-popcorn-plugins/freebase-search/freebase_logo.png"
      },
      {
        "type": "pausePlugin",
        "displayName": "Pause",
        "icon": "{{baseDir}}assets/plugins/pausePlugin/pause-icon.png",
        "targets": ["target-0"]
      },
      {
        "type": "comment",
        "displayName": "Comment",
        "icon": "{{baseDir}}assets/plugins/popup/popup-icon.png",
        "targets": ["target-1"]
      },
      {
        "type": "reference",
        "displayName": "Reference",
        "icon": "{{baseDir}}assets/plugins/text/text-icon.png",
        "targets": ["target-2"]
      },
      {
        "type": "interaction",
        "displayName": "Interaction",
        "targets": ["target-2"]
      },
      {
        "type": "imagePlugin",
        "displayName": "Image",
        "icon": "{{baseDir}}/../../hummedia-popcorn-plugins/imagePlugin/image.png",
        "targets": ["target-1", "target-2"]
      },
      {
        "type": "modal",
        "displayName": "Modal",
        "targets": ["target-0"]
      },
      {
        "type": "blank",
        "displayName": "Blank",
        "targets": ["target-0"]
      }
    ]
  },
  "dirs": {
    "css": "{{baseDir}}css/",
    "resources": "{{baseDir}}resources/"
  },
  "icons": {
    "default": "popcorn-icon.png",
    "image": "image-icon.png"
  }
}
